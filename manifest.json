{
    "manifest_version": 2,
    "name": "CSC411_Project_Implementation",
    "version": "1.0.0",
    "description": "SSO Manager",
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": { "scripts": ["background.js"] },
    "permissions": [
        "tabs",
        "cookies",
        "*://*/*",
        "<all_urls>"
    ]    
}